<div class = "users-new-wrapper">
  <div class = "container">
    <h1 class = "entry-name">ログイン</h1>
    <div class = "new-member-entry-form">
      <div class = "row">
        <div class = 'col-md-offset-3 col-md-6' >
          <%= form_for :session, url: login_path do |f| %>
            <div class = "form-group">
              <%= f.label :email,'メールアドレス', class: "new-label" %>
              <%= f.text_field :email, class: 'new-form'%>
            </div>
            <div class = "form-group">
              <%= f.label :password,'パスワード', class: 'new-label' %>
              <%= f.text_field :password, class: 'new-form' %>
            </div>
            <%= f.submit "登録", class: "btn-block"%>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>  
